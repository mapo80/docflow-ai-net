{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "e2e": "node run-e2e.mjs",
    "pree2e": "npm run build",
    "gen:api": "openapi --input ${VITE_API_BASE_URL:-.}/swagger/v1/swagger.json --output src/generated --client fetch --useOptions --useUnionTypes",
    "predev": "npm run gen:api",
    "prebuild": "npm run gen:api"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@uiw/react-json-view": "^2.0.0-alpha.35",
    "@uiw/react-markdown-preview": "^5.1.5",
    "@uiw/react-md-editor": "^4.0.3",
    "antd": "^5.27.0",
    "dayjs": "^1.11.13",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.0.0",
    "openapi-typescript-codegen": "^0.27.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^2.1.4"
  }
}
