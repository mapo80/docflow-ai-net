{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Information",
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Api": {
    "HeaderName": "X-API-Key",
    "Keys": [
      "dev-secret-key-change-me"
    ]
  },
  "LLM": {
    "ModelPath": "/models/qwen3-1_5-instruct-q4_0.gguf",
    "ContextTokens": 4096,
    "Threads": 6,
    "MaxTokens": 512,
    "Temperature": 0.2,
    "ThinkingMode": "auto",
    "UseGrammar": true
  },
  "BBox": {
    "DistanceAlgorithm": "BitParallel",
    "EditDistanceThreshold": 0.25,
    "MaxCandidates": 10,
    "EnableLabelProximity": true
  },
  "Resolver": {
    "Strategy": "TokenFirst",
    "TokenFirst": {
      "DistanceAlgorithm": "BitParallel",
      "EditDistanceThreshold": 0.25,
      "AdaptiveShortMax": 0.40,
      "AdaptiveLongMax": 0.35,
      "MaxCandidates": 10,
      "EnableLabelProximity": true
    }
  }
}