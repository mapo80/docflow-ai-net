{
  "defaultAction": "SCMP_ACT_ERRNO",
  "archMap": [
    {
      "architecture": "SCMP_ARCH_X86_64",
      "subArchitectures": [
        "SCMP_ARCH_X86",
        "SCMP_ARCH_X32"
      ]
    }
  ],
  "syscalls": [
    {
      "names": [
        "read",
        "write",
        "exit",
        "futex",
        "nanosleep",
        "clock_gettime",
        "rt_sigreturn",
        "rt_sigaction",
        "sigaltstack",
        "brk"
      ],
      "action": "SCMP_ACT_ALLOW"
    },
    {
      "names": [
        "recvfrom",
        "sendto",
        "socket",
        "connect",
        "getsockopt",
        "setsockopt",
        "shutdown"
      ],
      "action": "SCMP_ACT_ALLOW"
    },
    {
      "names": [
        "mmap",
        "mprotect",
        "munmap",
        "clone3",
        "clone",
        "getpid",
        "gettid",
        "tgkill",
        "set_robust_list"
      ],
      "action": "SCMP_ACT_ALLOW"
    },
    {
      "names": [
        "openat",
        "close",
        "fstat",
        "lseek",
        "access",
        "fcntl",
        "getrandom"
      ],
      "action": "SCMP_ACT_ALLOW"
    },
    {
      "names": [
        "statx",
        "newfstatat"
      ],
      "action": "SCMP_ACT_ALLOW"
    }
  ]
}